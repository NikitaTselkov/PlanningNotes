<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="black">#0F0F0F</Color>
    <SolidColorBrush x:Key="Black" Color="{StaticResource black}" />

    <Color x:Key="gray">#2A2834</Color>
    <SolidColorBrush x:Key="Gray" Color="{StaticResource gray}" />
    
    <Color x:Key="lightGray">#767381</Color>
    <SolidColorBrush x:Key="LightGray" Color="{StaticResource lightGray}" />
    
    <Color x:Key="darkGray">#1D1C23</Color>
    <SolidColorBrush x:Key="DarkGray" Color="{StaticResource darkGray}" />

    <Color x:Key="green">#00D486</Color>
    <SolidColorBrush x:Key="Green" Color="{StaticResource green}" />

    <Color x:Key="darkGreen">#0D1E18</Color>
    <SolidColorBrush x:Key="DarkGreen" Color="{StaticResource darkGreen}" />

    <Color x:Key="blue">#3E6A9F</Color>
    <SolidColorBrush x:Key="Blue" Color="{StaticResource blue}" />
    
</ResourceDictionary>